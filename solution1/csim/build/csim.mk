-include $(ObjDir)/dense.d

$(ObjDir)/eight.o: ../../../src/eight.cpp $(ObjDir)/.dir
	$(Echo) "   Compiling ../../../src/eight.cpp in $(BuildMode) mode" $(AVE_DIR_DLOG)
	$(Verb)  $(CC) ${CCFLAG} -c -MMD  $(IFLAG) $(DFLAG) $< -o $@ ; \

